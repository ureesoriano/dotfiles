{
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolPath": "off",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "editor.fontFamily": "Hack, Fira Code, Ubuntu Mono, DejaVu, D2Coding, Consolas, 'Courier New', monospace",
    "editor.fontWeight": "500",
    "editor.renderIndentGuides": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80
    ],
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "eslint.alwaysShowStatus": true,
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#000000",
    "vim.easymotionMarkerFontFamily": "Fira Code, Consolas, 'Courier New', monospace",
    "vim.easymotionMarkerForegroundColorTwoChar": "#E80E0E",
    "vim.easymotionMarkerForegroundColorOneChar": "#E80E0E",
    "vim.easymotionMarkerHeight": 19,
    "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf,.",
    "vim.hlsearch": true,
    "vim.handleKeys":{
        "<C-b>": false,
        "<C-c>": false,
        "<C-w>": false
    },
    "vim.insertModeKeyBindings": [
        { "before": ["º"], "after": ["<Esc>"] },
        { "before": ["ñ"], "after": [":"] }
    ],
    "vim.normalModeKeyBindings": [
        { "before": ["º"], "after": ["<Esc>"] },
        { "before": ["ñ"], "after": [":"] },
        { "before": [":", "q"], "after": ["workbench.files.action.closeFile"] },
        { "before": ["<S-h>"], "after": ["workbench.action.previousEditor"] },
        { "before": ["<S-l>"], "after": ["workbench.action.nextEditor"] }
    ],
    "vim.visualModeKeyBindings": [
        { "before": ["º"], "after": ["<Esc>"] },
        { "before": ["ñ"], "after": [":"] }
    ],
    "vim.leader": "ç",
    "vim.statusBarColorControl": true,
    "vim.searchHighlightColor": "rgb(231, 234, 75, 1)",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.enablePreview": false,
    "workbench.fontAliasing": "antialiased",
    "workbench.statusBar.feedback.visible": false,
    "window.zoomLevel": -1,
    "ycmd.enabled_languages": [
        "cpp",
        "c",
        "js"
    ]
}
